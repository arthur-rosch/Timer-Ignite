{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@rocketseat/eslint-config/react",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}